{
  "name": "owat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=4000 next dev",
    "build": "next build",
    "build:vercel": "bun db:migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
